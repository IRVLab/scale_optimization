<launch>

  <rosparam command="load" file="$(env HOME)/Workspace/data/cams/euroc/cam0.yaml" ns="/cam0"/>
  <rosparam command="load" file="$(env HOME)/Workspace/data/cams/euroc/cam1.yaml" ns="/cam1"/>

  <node pkg="so_svo" type="so_svo_node" name="so_svo_node" clear_params="true" output="screen">

    <!-- Camera topic to subscribe to -->
    <param name="cam0_topic" value="/cam0/image_raw" />
    <param name="cam1_topic" value="/cam1/image_raw" />

    <!-- Default parameter settings: choose between vo_fast and vo_accurate -->
    <rosparam file="$(find svo_ros)/param/vo_accurate.yaml" />

  </node>

</launch>
