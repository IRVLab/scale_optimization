<launch>

  <rosparam command="load" file="$(find dsvo_dso)/tests/cams/gazebo/T_stereo.yaml" ns="dsvo"/>

  <node name="dsvo" pkg="dsvo_dso" type="dsvo_node" output="screen">
    <param name="cam0_topic" value="/stereo/left/image_raw_color" />
    <param name="cam1_topic" value="/stereo/right/image_raw_color" />
    <param name="calib0" value="$(find dsvo_dso)/tests/cams/gazebo/camera0.txt" />
    <param name="calib1" value="$(find dsvo_dso)/tests/cams/gazebo/camera1.txt" />
    <param name="init_scale" value="1.2" />
    <param name="mode" value="1" />
    <param name="preset" value="0" />
    <param name="scale_accept_th" value="15" />
  </node>

  </launch>
